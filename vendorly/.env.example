# Vendorly Environment Configuration

# Google Gemini AI API (for AI Chat Service)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Razorpay Payment Gateway (for payment processing)
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id_here
VITE_RAZORPAY_KEY_SECRET=your_razorpay_key_secret_here

# Firebase Configuration (for push notifications)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_VAPID_KEY=your_vapid_key_here

# Supabase Configuration (for database and authentication)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Maps API (for location services)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# SMS Service (for OTP verification)
VITE_SMS_API_KEY=your_sms_api_key_here
VITE_SMS_SENDER_ID=VENDORLY

# Analytics and Monitoring
VITE_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
VITE_SENTRY_DSN=your_sentry_dsn_here

# App Configuration
VITE_APP_NAME=Vendorly
VITE_APP_VERSION=1.0.0
VITE_API_BASE_URL=https://api.vendorly.in
VITE_CDN_BASE_URL=https://cdn.vendorly.in

# Feature Flags
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PAYMENT_GATEWAY=true
VITE_ENABLE_VOICE_SEARCH=true
VITE_ENABLE_BARCODE_SCANNER=true

# Development Settings
VITE_DEV_MODE=true
VITE_DEBUG_LEVEL=info
VITE_MOCK_PAYMENTS=true